{
    "name": "Hide Site History",
    "version": "0.0.1",
    "description": "Test version of extension for browser for hiding history of sites in the list",
    "permissions": ["tabs", "storage", "history"],
    "background": {
        "scripts": ["src/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "src/popup/popup.html"
    },
    "icons": {
        "128": "src/images/edit-tools.png"
    },
    "manifest_version": 2
}